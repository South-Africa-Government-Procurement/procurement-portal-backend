{
  "stack": "container",
  "environments": {
    "review": {
      "addons": ["heroku-postgresql:hobby-dev"],
      "env": {
        "TAG_MANAGER_ENABLED": "False",
        "DEFAULT_FILE_STORAGE": "django.core.files.storage.FileSystemStorage"
      },
      "scripts": {
        "postdeploy": "python manage.py flush --no-input && python manage.py migrate && python manage.py loaddata demodata"
      }
    }
  }
}
